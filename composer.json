{
    "name": "zfcampus/zf-apigility-welcome",
    "description": "Apigility Welcome Screen",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf",
        "zendframework",
        "api",
        "apigility",
        "framework"
    ],
    "support": {
        "issues": "https://github.com/zfcampus/zf-apigility-welcome/issues",
        "source": "https://github.com/zfcampus/zf-apigility-welcome",
        "rss": "https://github.com/zfcampus/zf-apigility-welcome/releases.atom",
        "chat": "https://zendframework-slack.herokuapp.com",
        "forum": "https://discourse.zendframework.com/c/questions/apigility"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-mvc": "^2.7.15 || ^3.0.4",
        "zendframework/zend-view": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27 || ^6.5.8 || ^7.1.5",
        "zendframework/zend-coding-standard": "~1.0.0",
        "zendframework/zend-i18n": "^2.7.4",
        "zendframework/zend-log": "^2.9.2",
        "zendframework/zend-modulemanager": "^2.8",
        "zendframework/zend-serializer": "^2.8",
        "zendframework/zend-test": "^2.6.1 || ^3.1"
    },
    "autoload": {
        "psr-4": {
            "ZF\\Apigility\\Welcome\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\Apigility\\Welcome\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev",
            "dev-develop": "1.2.x-dev"
        },
        "zf": {
            "module": "ZF\\Apigility\\Welcome"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    }
}
